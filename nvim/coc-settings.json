{
  "suggest.noselect":false,
  "suggest.preferCompleteThanJumpPlaceholder":true,
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlint": true
        }
      }
    }
  },
  "prettier.enable": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "javascript"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "stylelintplus.filetypes": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "sugarss",
    "vue",
    "wxss"
  ],
  "stylelintplus.autoFixOnFormat": true,
  "stylelintplus.autoFixOnSave": true
}
