{
  "suggest.noselect":false,
  "suggest.preferCompleteThanJumpPlaceholder":true,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlint": true
        }
      }
    }
  },
  "sql.database": "mysql",
  "prettier.enable": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "javascript"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "stylelintplus.filetypes": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "sugarss",
    "vue",
    "wxss"
  ],
  "stylelintplus.autoFixOnFormat": true,
  "stylelintplus.autoFixOnSave": true,
  "jedi": {
    "enable": true,
    "startupMessage": false,
    "executable.command": "~/.asdf/installs/python/3.11.2/bin/jedi-language-server"
  }
}
